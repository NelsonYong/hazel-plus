{
  "name": "hazel-plus",
  "private": false,
  "version": "0.0.4",
  "main": "dist/hooks/index.es.js",
  "module": "dist/hooks/index.cjs.js",
  "types": "dist/types/packages/hooks/index.d.ts",
  "type": "commonjs",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "initial": "npx initial-scan .docs --srcDir packages ",
    "docs:dev": "initial-scan && vitepress dev .docs --srcDir packages",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "type": "vue-tsc",
    "preview": "vite preview",
    "eslint": "eslint src --ext .ts"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@ruabick/md-demo-plugins": "^0.2.4",
    "@ruabick/vite-plugin-gen-api-doc": "^0.2.4",
    "@ruabick/vite-plugin-gen-temp": "^0.2.4",
    "@ruabick/vitepress-demo-block": "^0.2.4",
    "@types/node": "^18.7.9",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue-hooks-plus/md-demo-plugins": "^1.0.0",
    "@vue-hooks-plus/vite-plugin-gen-temp": "^1.1.0",
    "@vue-hooks-plus/vitepress-demo-block": "^1.0.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "hazel-plus": "^0.0.4",
    "prettier": "^2.7.1",
    "sass": "^1.54.4",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-plugin-dts": "^1.4.1",
    "vitepress": "1.0.0-alpha.9",
    "vue-router": "^4.1.3",
    "vue-tsc": "^0.38.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
